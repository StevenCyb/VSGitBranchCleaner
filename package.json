{
	"name": "VSGitBranchCleaner",
	"displayName": "VSGitBranchCleaner",
	"description": "Clean local branches",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.56.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:VSGitBranchCleaner.run"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "VSGitBranchCleaner.run",
				"title": "Clean local branches"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"lint": "eslint . --ext .ts,.tsx"
	},
	"devDependencies": {
		"@types/node": "^12.12.0",
		"@types/vscode": "^1.32.0",
		"@typescript-eslint/eslint-plugin": "^4.16.0",
		"@typescript-eslint/parser": "^4.16.0",
		"eslint": "^7.21.0",
		"typescript": "^4.2.2"
	}
}
